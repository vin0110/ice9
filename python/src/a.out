* begin write string
   3:    JEQ 1,5(7)	write(str): test len == 0
   4:     LD 3,0(2)	r3 = *sp
   5:   OUTC 3,3,3	out *sp
   6:    LDA 2,1(2)	++sp
   7:    LDA 1,-1(1)	--len
